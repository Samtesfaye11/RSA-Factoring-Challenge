import math
import sys

def factorize_numbers(filename):
    with open(filename, 'r') as file:
        for line in file:
            number = int(line.strip())
            for factor in range(2, int(math.sqrt(number)) + 1):
                if number % factor == 0:
                    divisor = number // factor
                    print(f"{number}={factor}*{divisor}")
                    break

if __name__ == '__main__':
    if len(sys.argv) != 2:
        print("Usage: factors <file>")
    else:
        filename = sys.argv[1]
        factorize_numbers(filename)
